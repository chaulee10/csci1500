//NOT FOLLOWING INSTRUCTIONS

#include <iostream>
#include <iomanip>
using namespace std;

int main()
{
    int fib[20];
    double ratio;

    fib[0]=1;
    fib[1] = 1;
    
    cout.setf(ios::fixed);
    cout.setf(ios::showpoint);
    cout.precision(8);
    
    cout << "FIB#" << setw(1) << "RATIO" << endl;
    for (int i=2; i<20; i++)
    { cout << (fib[i] = fib[i-1]+fib[i-2]) << endl; }

    for (int j=2;j<20; j++)
    {
        ratio = (double) fib[j]/fib[j-1];
        cout << setw(20) << ratio << endl;
    }
    
    return 0;
}
