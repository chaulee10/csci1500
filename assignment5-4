#include <iostream>
using namespace std;


int main(){
	int table[3][3], sum1 = 0,sum2 = 0,sum3 = 0;

    cout <<"Enter row 1: ";
    for (int i=0; i<3;i++)
    {  cin >> table[0][i];}

    cout <<"Enter row 2: ";
    for (int i=0; i<3;i++)
    {  cin >> table[1][i];}

    cout <<"Enter row 1: ";
    for (int i=0; i<3;i++)
    {  cin >> table[2][i];}

    cout << "Table: \n";
    for (int row=0; row < 3; row++)
    {
        for (int column=0;column<3; column++)
        {cout << table[row][column] << " ";}
        cout<<endl;
    }

    cout << "-------------- \n";

    sum1 = table[0][0]+table[1][0]+table[2][0];
    sum2 = table[1][0]+table[1][1]+table[1][2];
	sum3 = table[2][0]+table[2][1]+table[2][2];

	cout << sum1 << " " <<  sum2 << " " <<  sum3 << endl;

	return 0;
}
